{
    icon_theme: vscode
    verbs: [
        {
            invocation: edit
            shortcut: e
            execution: "code --goto {file}:{line}"
            leave_broot: false
        }
        {
            invocation: terminal
            key: ctrl-t
            execution: "$SHELL"
            set_working_dir: true
            leave_broot: false
        }
    ]
}
