#!/usr/bin/env bash

input=$(cat)
if [[ -z "$input" ]]; then
  export AWS_PROFILE=mpdx-amplify
  aws sts get-caller-identity > /dev/null 2>&1 || aws sso login
  aws amplify get-app --app-id d3dytjb8adxkk5 | jq --raw-output '.app.environmentVariables | to_entries | .[] | "\(.key)=\(.value)"'
else
  printf '%s' "$input"
fi
